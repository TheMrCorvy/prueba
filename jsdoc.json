{
	"source": {
		"include": ["src"],
		"includePattern": "\\.(jsx|js|ts|tsx)$",
		"excludePattern": "(node_modules|docs)"
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"opts": {
		"destination": "./docs",
		"recurse": true,
		"template": "node_modules/better-docs/"
	},
	"tags": {
		"allowUnknownTags": ["optional"]
	},
	"plugins": ["node_modules/better-docs/typescript"]
}
